  <property name="styleSheet">
   <string notr="true">QLabel {
    color: white;
    font-size: 14px;
    font-weight: 500;
    letter-spacing: 0.5px;
}

/* Hilangkan underline &amp; warna biru pada QLabel */
QLabel {
    text-decoration: none;
    color: white;
}

QLineEdit {
    background: rgba(255,255,255,0.9);
    border: 2px solid transparent;
    border-radius: 12px;
    padding: 10px 14px;
    font-size: 16px;

    /* simple shadow */
    box-shadow: 0px 4px 12px rgba(0,0,0,0.35);
}

QLineEdit:focus {
    border: 2px solid #D50000;
    background: rgba(255,255,255,1);
    box-shadow: 0px 6px 18px rgba(0,0,0,0.45);
}

/* ====== PUSH BUTTON (Login, Daftar, dll) ====== */
QPushButton {
    background-color: #D50000;
    color: white;
    border-radius: 14px;
    padding: 12px 20px;
    font-size: 16px;
    font-weight: bold;
    border: none;

    /* Drop shadow simple */
    box-shadow: 0px 4px 10px rgba(0,0,0,0.35);
}

QPushButton:hover {
    background-color: #b40000;
    box-shadow: 0px 6px 14px rgba(0,0,0,0.45);
}
#pushButton {
    background-color: #D50000;
    color: white;
    border-radius: 14px;
    padding: 12px 20px;
    border: none;
    font-size: 17px;
    font-weight: bold;

    /* Shadow */
    box-shadow: 0px 5px 16px rgba(0,0,0,0.40);
}

#pushButton:hover {
    background-color: #b40000;
    box-shadow: 0px 7px 20px rgba(0,0,0,0.50);
    letter-spacing: 1px; /* efek modern */
}

#pushButton:pressed {
    background-color: #8e0000;
    box-shadow: 0px 3px 10px rgba(0,0,0,0.30);
    padding-top: 14px;
    padding-bottom: 10px;
}

#pushButton_2 {
    background-color: transparent;
    border: none;
    color: #1E88E5;
    font-size: 16px;
    text-decoration: underline;
    font-weight: 600;
}

#pushButton_2:hover {
    color: #64B5F6;
    cursor: pointer;
    text-decoration: underline;
    letter-spacing: 0.5px;
}

#pushButton_2:pressed {
    color: #0D47A1;
}
</string>
